use_frameworks!

target 'ELColorGame' do
  # UI
  pod 'SnapKit', '~> 3.0.0'
  pod 'Spring', :git => 'https://github.com/MengTo/Spring.git', :branch => 'swift3'
  
  # External analytic services
  pod 'Crashlytics', '~> 3.6'
  pod 'Fabric', '~> 1.6'
  pod 'FBSDKCoreKit', '~> 4.21.0'
  pod 'GA-SDK-IOS'
  pod 'Google/Analytics'
end

target 'ELColorGameTests' do
  pod 'Google/Analytics'
  pod 'Nimble'
  pod 'Quick'
end
